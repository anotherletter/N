{
  "name": "n",
  "version": "0.0.0",
  "description": "A minimal Notes App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "babel src --out-dir dist",
    "icon": "nicns --in ./assets/Icon1024.png --out ./assets/icon.icns",
    "prepack": "npm run build && npm run icon",
    "pack": "electron-packager . N --overwrite --icon ./assets/icon.icns",
    "reduce-deps": "node ./scripts/reduce-dependencies.js",
    "postinstall": "npm run reduce-deps"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "electron": "~1.7.8",
    "electron-packager": "^9.1.0",
    "node-icns": "0.0.4"
  },
  "dependencies": {
    "@atlaskit/button": "^5.4.2",
    "@atlaskit/css-reset": "^1.1.5",
    "@atlaskit/editor-core": "^53.1.4",
    "@atlaskit/icon": "^10.1.0",
    "date-fns": "^1.29.0",
    "electron-default-menu": "^1.0.1",
    "electron-store": "^1.3.0",
    "electron-window-state": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^15.5.0",
    "react-dom": "^15.6.2",
    "rxjs": "^5.4.3",
    "styled-components": "^1.4.6",
    "url-search-params": "^0.10.0"
  }
}
